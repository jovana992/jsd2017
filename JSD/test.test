model Korisnik {
    agencija = ForeignKey()
    ime = CharField(max_length = 10)
    prezime = CharField(max_length = 15)
    adresa = CharField(max_length = 50)
    jmbg = IntegerField()
    email = EmailField(max_length = 64)
    pol = BooleanField()
};

model Agencija {
    naziv = CharField(max_length = 100)
    adresa = CharField(max_length = 50)
    telefon = IntegerField()
};
